<%- layout("./client/layouts/layout") %>

<div class="row">
   <% for (let p of products) { %>
      <div class="col-md-2 product-item">
         <img src="" alt="">
         <h6>
            <%= p.name %>
         </h6>
         <div>Php <%= p.price %></div>
         <div>
            <img src="/img/star-rating/<%= p.rating %>.png" width="86"/>
         </div>
         <div>
            <%= p.sold %> sold
         </div>
      </div>
   <% } %>
   
</div>